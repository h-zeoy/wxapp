<!--index.wxml-->
<view class="home-wrap">
  <block wx:for="{{ audioList }}" wx:key="moduleId">
    <list-item
      title="{{ item.title }}"
      type="{{ item.type }}"
      list="{{ item.list }}"
      moduleId="{{ item.moduleId }}"
      bind:listItemAudioEvent="listItemAudioEvent" 
    />
  </block>
  <view class="footer-title">找回内心的平静与幸福</view>
</view>
